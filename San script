local UtilityTab = Window:CreateTab("Utility", 4483362458) 

UtilityTab:CreateButton({
   Name = "Destroy San Hud",
   Callback = function()
       Rayfield:Notify({Title = "Destroy", Content = "ปิด UI สำเร็จ", Duration = 3, Image = 4483362458})
       Rayfield:Destroy()
   end,
})

UtilityTab:CreateButton({
   Name = "God Mode (100M HP)",
   Callback = function()
       Rayfield:Notify({Title = "God Mode", Content = "กำลังเปิด God Mode (100M HP) - จะรีเซ็ตเมื่อตาย", Duration = 5, Image = 4483362458})
       
       local DESIRED_HEALTH = 100000000 
       local Player = game.Players.LocalPlayer
       local function setCharacterHealth(character)
           local humanoid = character:WaitForChild("Humanoid")
           humanoid.MaxHealth = DESIRED_HEALTH
           task.spawn(function()
               while task.wait(0.1) do
                   if not humanoid or humanoid.Parent == nil then break end
                   if humanoid.MaxHealth ~= DESIRED_HEALTH then humanoid.MaxHealth = DESIRED_HEALTH end
                   if humanoid.Health ~= DESIRED_HEALTH then humanoid.Health = DESIRED_HEALTH end
               end
           end)
       end
       Player.CharacterAdded:Connect(setCharacterHealth)
       if Player.Character then setCharacterHealth(Player.Character) end
       print("Modified Health Bypass (100M) Initialized.")
   end,
})

UtilityTab:CreateButton({
   Name = "San Climb Script",
   Callback = function()
       notifyLoading("San Climb")
       loadstring(game:HttpGet('https://raw.githubusercontent.com/sannnn530/San/refs/heads/main/San%20climb'))()
   end,
})
