-- 1. ‡πÇ‡∏´‡∏•‡∏î Rayfield Library
local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

-- 2. ‡∏ï‡∏±‡πâ‡∏á‡∏Ñ‡πà‡∏≤‡πÅ‡∏•‡∏∞‡πÄ‡∏õ‡∏¥‡∏î‡πÉ‡∏ä‡πâ‡∏á‡∏≤‡∏ô Key System 
Rayfield:SetKeySystem({
    Title = "key", 
    Subtitle = "Key System",
    Note = "find in https://discord.gg/UQWAh5pd5f",
    FileName = "Key",
    SaveKey = true,
    GrabKeyFromSite = true,
    Key = {"https://pastebin.com/raw/ZJvpk3Rv"}
})

-- 3. ‡∏™‡∏£‡πâ‡∏≤‡∏á Window
local Window = Rayfield:CreateWindow({
    Name = "SanScriptHub",
    Icon = 0,
    LoadingTitle = "Sanscripthud is loading",
    LoadingSubtitle = "by San 11year old",
    ShowText = "San script hud",
    Theme = "Default",
    ToggleUIKeybind = "K",
    DisableRayfieldPrompts = false,
    DisableBuildWarnings = false,

    ConfigurationSaving = {
        Enabled = true,
        FolderName = "San Hub Configs",
        FileName = "San script Hub"
    },

    Discord = {
        Enabled = true,
        Invite = "UQWAh5pd5f",
        RememberJoins = false
    }
})

