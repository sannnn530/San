### üåô Tab 99 Night

local Tab99Night = Window:CreateTab("99night", 4483362458)
-- ... (‡∏õ‡∏∏‡πà‡∏°‡∏ï‡πà‡∏≤‡∏á‡πÜ ‡∏Ç‡∏≠‡∏á 99 Night) ...
local ButtonVape = Tab99Night:CreateButton({
   Name = "vape void ware",
   Callback = function()
      loadstring(game:HttpGet("https://raw.githubusercontent.com/VapeVoidware/VW-Add/main/nightsintheforest.lua", true))()
      Rayfield:Notify({ Title = "99 Night", Content = "Vape Void Ware loaded!", Duration = 4, Image = "check-circle" })
   end,
})

local ButtonThai = Tab99Night:CreateButton({
   Name = "Thai",
   Callback = function()
      loadstring(game:HttpGet('https://raw.githubusercontent.com/MQPS7/99-Night-in-the-Forset/refs/heads/main/99Nightv1'))()
