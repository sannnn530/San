### üõ†Ô∏è Tab General Tools (‡∏£‡∏ß‡∏° Fly ‡πÅ‡∏•‡∏∞ Auto Clicker)
local TabTools = Window:CreateTab("General Tools", 4483362458) 

-- 4.1 Section: Fly
TabTools:CreateSection("Movement Tools")
local ButtonFlyGui = TabTools:CreateButton({
   Name = "XNEOFF FlyGuiV3",
   Callback = function()
      loadstring(game:HttpGet("https://raw.githubusercontent.com/XNEOFF/FlyGuiV3/main/FlyGuiV3.txt"))() 
      Rayfield:Notify({ Title = "Fly Tab", Content = "FlyGuiV3 loaded!", Duration = 4, Image = "trending-up" })
   end,
})

-- 4.2 Section: Auto Clicker
TabTools:CreateSection("Automation")
local ButtonAutoClicker = TabTools:CreateButton({
   Name = "Hostile Auto Clicker (Keyed)",
   Callback = function()
      getgenv().key = "Hostile"
      loadstring(game:HttpGet("https://raw.githubusercontent.com/Hosvile/The-telligence/main/MC%20KSystem%202"))()
      Rayfield:Notify({ Title = "Auto Clicker Tab", Content = "Hostile Auto Clicker loaded!", Duration = 4, Image = "mouse-pointer" })
   end,
})
